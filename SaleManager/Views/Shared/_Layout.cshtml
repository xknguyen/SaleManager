<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ETesting</title>
    @Styles.Render("~/Content/admbootstrap")
    @Styles.Render("~/Content/admstyle")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("pluginStyles", required: false)
</head>
<body>
<div id="wrapper">
    @Html.Partial("_Navigation")
    <div id="page-wrapper" class="gray-bg">
        <div class="row border-bottom">
            @Html.Partial("_NavBarHeader")
        </div>

        <div class="row wrapper border-bottom white-bg page-heading">
            @Html.Partial("_PageHeader")
        </div>

        <div class="wrapper wrapper-content">
            @RenderBody()
        </div>

        <footer>
            @Html.Partial("_Footer")
        </footer>
    </div>
</div>
    @Scripts.Render("~/bundles/admmainjs")
    @RenderSection("pluginScripts", required:false)
    @Scripts.Render("~/bundles/admcustomjs")
    @RenderSection("scripts", required: false)
</body>
</html>